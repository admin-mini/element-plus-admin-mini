{
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["element-plus/global"],
    "lib": ["ESNext", "DOM"],
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src/**/*"]
}
